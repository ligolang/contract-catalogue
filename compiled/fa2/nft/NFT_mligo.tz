{ parameter
    (or (list %transfer
           (pair (address %from_) (list %tx (pair (address %to_) (nat %token_id)))))
        (or (pair %balance_of
               (list %requests (pair (address %owner) (nat %token_id)))
               (contract %callback
                  (list (pair (pair %request (address %owner) (nat %token_id)) (nat %balance)))))
            (list %update_operators
               (or (pair %add_operator (address %owner) (address %operator) (nat %token_id))
                   (pair %remove_operator (address %owner) (address %operator) (nat %token_id)))))) ;
  storage
    (pair (pair (big_map %ledger nat address)
                (big_map %operators (pair address address) (set nat)))
          (list %token_ids nat)
          (big_map %token_metadata nat (pair (nat %token_id) (map %token_info string bytes)))) ;
  code { LAMBDA (pair address address) bool { UNPAIR ; COMPARE ; EQ } ;
         LAMBDA
           (pair (lambda (pair address address) bool) address)
           unit
           { UNPAIR ;
             SWAP ;
             SENDER ;
             SWAP ;
             PAIR ;
             EXEC ;
             PUSH string "The sender can only manage operators for his own token" ;
             SWAP ;
             IF { DROP ; UNIT } { FAILWITH } } ;
         DUP 2 ;
         APPLY ;
         LAMBDA
           (pair (lambda (pair address address) bool) (pair (big_map nat address) nat address))
           bool
           { UNPAIR ;
             SWAP ;
             UNPAIR 3 ;
             SWAP ;
             GET ;
             IF_NONE { PUSH string "option is None" ; FAILWITH } {} ;
             PAIR ;
             EXEC } ;
         DUP 3 ;
         APPLY ;
         LAMBDA
           (pair (pair (pair (big_map nat address) (big_map (pair address address) (set nat)))
                       (list nat)
                       (big_map nat (pair nat (map string bytes))))
                 nat)
           unit
           { UNPAIR ;
             PUSH string "FA2_TOKEN_UNDEFINED" ;
             SWAP ;
             CDR ;
             CDR ;
             DIG 2 ;
             GET ;
             IF_NONE { FAILWITH } { DROP 2 } ;
             UNIT } ;
         DIG 4 ;
         UNPAIR ;
         IF_LEFT
           { DIG 4 ;
             DROP ;
             DUP 2 ;
             CAR ;
             CAR ;
             SWAP ;
             ITER { UNPAIR ;
                    DUG 2 ;
                    ITER { UNPAIR ;
                           DUP 2 ;
                           DUP 6 ;
                           PAIR ;
                           DUP 7 ;
                           SWAP ;
                           EXEC ;
                           DROP ;
                           SENDER ;
                           DUP 5 ;
                           DUP 2 ;
                           PAIR ;
                           DUP 10 ;
                           SWAP ;
                           EXEC ;
                           IF { DROP }
                              { DUP 6 ;
                                CAR ;
                                CDR ;
                                SWAP ;
                                DUP 6 ;
                                PAIR ;
                                GET ;
                                IF_NONE { EMPTY_SET nat } {} ;
                                DUP 3 ;
                                MEM ;
                                IF {} { PUSH string "FA2_NOT_OPERATOR" ; FAILWITH } } ;
                           DUP 4 ;
                           DUP 3 ;
                           DUP 5 ;
                           PAIR 3 ;
                           DUP 8 ;
                           SWAP ;
                           EXEC ;
                           PUSH string "FA2_INSUFFICIENT_BALANCE" ;
                           SWAP ;
                           IF { DROP } { FAILWITH } ;
                           DIG 2 ;
                           SWAP ;
                           SOME ;
                           DIG 2 ;
                           UPDATE } ;
                    SWAP ;
                    DROP } ;
             DIG 2 ;
             DIG 3 ;
             DIG 4 ;
             DROP 3 ;
             DUP 2 ;
             CDR ;
             DIG 2 ;
             CAR ;
             CDR ;
             DIG 2 ;
             PAIR ;
             PAIR ;
             NIL operation }
           { DIG 5 ;
             DROP ;
             IF_LEFT
               { DIG 4 ;
                 DROP ;
                 UNPAIR ;
                 MAP { DUP ;
                       UNPAIR ;
                       DUP 2 ;
                       DUP 6 ;
                       PAIR ;
                       DUP 7 ;
                       SWAP ;
                       EXEC ;
                       DROP ;
                       SWAP ;
                       DUP 5 ;
                       CAR ;
                       CAR ;
                       PAIR 3 ;
                       DUP 6 ;
                       SWAP ;
                       EXEC ;
                       IF { PUSH nat 1 } { PUSH nat 0 } ;
                       SWAP ;
                       PAIR } ;
                 DIG 3 ;
                 DIG 4 ;
                 DROP 2 ;
                 SWAP ;
                 PUSH mutez 0 ;
                 DIG 2 ;
                 TRANSFER_TOKENS ;
                 SWAP ;
                 NIL operation ;
                 DIG 2 ;
                 CONS }
               { DIG 2 ;
                 DIG 3 ;
                 DROP 2 ;
                 DUP 2 ;
                 CAR ;
                 CDR ;
                 SWAP ;
                 ITER { IF_LEFT
                          { UNPAIR 3 ;
                            DUP 2 ;
                            DUP 2 ;
                            COMPARE ;
                            EQ ;
                            IF { DROP 3 }
                               { DUP ;
                                 DUP 7 ;
                                 SWAP ;
                                 EXEC ;
                                 DROP ;
                                 DUP 4 ;
                                 DIG 4 ;
                                 DUP 4 ;
                                 DUP 4 ;
                                 PAIR ;
                                 GET ;
                                 IF_NONE { EMPTY_SET nat } {} ;
                                 DIG 4 ;
                                 PUSH bool True ;
                                 SWAP ;
                                 UPDATE ;
                                 SOME ;
                                 DIG 3 ;
                                 DIG 3 ;
                                 PAIR ;
                                 UPDATE } }
                          { UNPAIR 3 ;
                            DUP 2 ;
                            DUP 2 ;
                            COMPARE ;
                            EQ ;
                            IF { DROP 3 }
                               { DUP ;
                                 DUP 7 ;
                                 SWAP ;
                                 EXEC ;
                                 DROP ;
                                 DUP 4 ;
                                 DUP 3 ;
                                 DUP 3 ;
                                 PAIR ;
                                 GET ;
                                 IF_NONE
                                   { DIG 2 ; DROP ; NONE (set nat) }
                                   { DIG 3 ;
                                     PUSH bool False ;
                                     SWAP ;
                                     UPDATE ;
                                     PUSH nat 0 ;
                                     DUP 2 ;
                                     SIZE ;
                                     COMPARE ;
                                     EQ ;
                                     IF { DROP ; NONE (set nat) } { SOME } } ;
                                 DIG 3 ;
                                 SWAP ;
                                 DIG 3 ;
                                 DIG 3 ;
                                 PAIR ;
                                 UPDATE } } } ;
                 DIG 2 ;
                 DROP ;
                 DUP 2 ;
                 CDR ;
                 SWAP ;
                 DIG 2 ;
                 CAR ;
                 CAR ;
                 PAIR ;
                 PAIR ;
                 NIL operation } } ;
         PAIR } ;
  view "get_balance"
       (pair address nat)
       nat
       { UNPAIR ;
         UNPAIR ;
         PUSH string "FA2_TOKEN_UNDEFINED" ;
         DUP 4 ;
         CDR ;
         CDR ;
         DUP 4 ;
         GET ;
         IF_NONE { FAILWITH } { DROP 2 } ;
         DIG 2 ;
         CAR ;
         CAR ;
         DIG 2 ;
         GET ;
         IF_NONE { PUSH string "option is None" ; FAILWITH } {} ;
         COMPARE ;
         EQ ;
         IF { PUSH nat 1 } { PUSH nat 0 } } ;
  view "total_supply"
       nat
       nat
       { UNPAIR ;
         PUSH string "FA2_TOKEN_UNDEFINED" ;
         DIG 2 ;
         CDR ;
         CDR ;
         DIG 2 ;
         GET ;
         IF_NONE { FAILWITH } { DROP 2 } ;
         PUSH nat 1 } ;
  view "all_tokens" unit (list nat) { CDR ; CDR ; CAR } ;
  view "is_operator"
       (pair (address %owner) (address %operator) (nat %token_id))
       bool
       { UNPAIR ;
         DUP ;
         CAR ;
         DUP 2 ;
         GET 3 ;
         DIG 3 ;
         CAR ;
         CDR ;
         DUP 2 ;
         DUP 4 ;
         PAIR ;
         GET ;
         IF_NONE { EMPTY_SET nat } {} ;
         DIG 3 ;
         GET 4 ;
         MEM ;
         SWAP ;
         DIG 2 ;
         COMPARE ;
         EQ ;
         OR } ;
  view "token_metadata"
       nat
       (pair (nat %token_id) (map %token_info string bytes))
       { UNPAIR ;
         SWAP ;
         CDR ;
         CDR ;
         SWAP ;
         GET ;
         IF_NONE { PUSH string "FA2_TOKEN_UNDEFINED" ; FAILWITH } {} } }

